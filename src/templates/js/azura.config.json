{
  "environment": "development",
  "server": {
    "port": 3000,
    "cluster": false,
    "ipHost": false,
    "https": false,
    "trustProxy": true
  },
  "plugins": {
    "rateLimit": {
      "enabled": true,
      "limit": 100,
      "timeframe": 60000
    },
    "cors": {
      "enabled": true,
      "origins": ["*"],
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"],
      "allowedHeaders": ["Content-Type", "Authorization"]
    }
  },
  "logging": {
    "enabled": true,
    "showDetails": true
  }
}
